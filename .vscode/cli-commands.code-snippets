            "\t\traise typer.Exit()",
            "\tconsole = rich.console.Console()",
            "\ttable = rich.table.Table(\"id\", \"creado\", \"inicio\", \"col_str\", \"col_int\", \"col_bool\")",
            "\tfor registro in respuesta[\"items\"]:",
            "\t\tcreado = datetime.strptime(registro[\"creado\"], \"%Y-%m-%dT%H:%M:%S.%f\")",
